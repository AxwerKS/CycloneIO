{
  	"expo": {
		"name": "Cyclone",
		"slug": "mobile",
		"privacy": "public",
		"sdkVersion": "32.0.0",

		"entryPoint": "source/index.js",
		
		"platforms": [
			"ios",
			"android"
		],

		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./source/assets/icon.png",

		"splash": {
			"image": "./source/assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},

		"updates": {
			"fallbackToCacheTimeout": 0
		},

		"assetBundlePatterns": [
			"**/*"
		],

		"ios": {
			"supportsTablet": true
		}
	}
}