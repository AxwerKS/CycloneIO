  "main": "index.js",
  "scripts": {
    "build": "npm run server:build && npm run build:resources && cp source/network/constants.json dist/network/",
    "server:build": "babel source --out-dir dist --ignore 'source/web' --ignore 'source/client'",
    "build:dev": "flow status --quiet && babel-watch --watch source source/environment.js --exclude 'source/web' --exclude 'source/client'",
    "build:resources": "webpack --mode=production",
    "dev:resources": "webpack-dev-server --host 0.0.0.0",
    "desktop": "electron ./resources/desktop/index.js",
    "dev:housekeeping": "webpack-dev-server --host 0.0.0.0 --open --config ./source/housekeeping/webpack.config.babel.js",
    "dev:ui:r63b": "webpack-dev-server --host 0.0.0.0 --open --config ./resources/ui/r63b/webpack.config.babel.js",
    "dev:ui:r63b:cypress": "cypress open",
    "flow:status": "flow status --quiet"
  },
