{
  "name": "cyclone",
  "version": "1.0.0",
  "description": "![](https://media.discordapp.net/attachments/557261127847772161/557591227709521922/1803003194_edd50080-e5ee-4490-8da1-b7927a8383a6.png)",
  "main": "index.js",
  "scripts": {
    "apollo:schema": "apollo client:download-schema --endpoint http://localhost:8081/graphql ./packages/storage/schema.json",
    "apollo:types": "apollo client:codegen --includes ./packages/storage/queries.ts --localSchemaFile ./packages/storage/schema.json --target typescript --outputFlat ./packages/storage/__generated__/types.ts",
    "build": "npm run server:build",
    "server:build": "babel source --out-dir dist --ignore 'source/web' --ignore 'source/client'",
    "build:resources": "webpack --mode=production",
    "desktop": "electron ./resources/desktop/index.js",
    "dev:web": "webpack-dev-server --host 0.0.0.0 --open --config ./packages/web/webpack.config.babel.js",
    "dev:housekeeping": "webpack-dev-server --host 0.0.0.0 --open --config ./packages/housekeeping/webpack.config.babel.js",
    "dev:ui:r63b:cypress": "cypress open",
    "swf:utils:furniture": "babel-watch source/utils/swf/index.js",
    "swf:utils:imager": "babel-watch source/utils/imager/index.js",
    "dev:ui:r26": "webpack-dev-server --host 0.0.0.0 --open --config ./packages/client-themes/R26/webpack.config.babel.js",
    "dev:ui:r63b": "webpack-dev-server --public=http://localhost:8082 --host 0.0.0.0 --open --config ./packages/client-themes/r63b/webpack.config.babel.js",
    "dev:ui:mobile": "webpack-dev-server --host 0.0.0.0 --open --config ./packages/client-themes/mobile/webpack.config.babel.js",
    "dev:ui:mobile:ios": "cd source/mobile && react-native run-ios --simulator=\"iPhone10\"",
    "dev:server": "babel-watch --watch source/server source/server/environment.ts --extensions \".ts,.tsx\"",
    "demo:server": "webpack-dev-server --host 0.0.0.0 --open --config ./packages/client-themes/r63b/demo.config.babel.js",
    "demo": "webpack --mode=production --config ./packages/client-themes/r63b/demo.config.babel.js",
    "dev:resources": "webpack-dev-server --host 0.0.0.0",
    "lerna:test": "lerna run test",
    "test": "jest --watchAll",
    "ref:": "tsc ./packages/server/environment.ts --noEmit",
    "lerna:dev:server": "lerna run dev --parallel --scope @cyclone/server",
    "lerna:dev:housekeeping": "lerna run dev --parallel --scope @cyclone/housekeeping",
    "lerna:dev:desktop": "lerna run dev --parallel --scope @cyclone/desktop",
    "lerna:dev:mobile": "lerna run android --parallel --scope @cyclone/mobile --scope @cyclone/client-theme-m01",
    "lerna:dev:mobile:ios": "lerna run ios --parallel --scope @cyclone/mobile --scope @cyclone/client-theme-m01",
    "lerna:dev:ui:r26": "lerna run dev --parallel --scope @cyclone/client-theme-r26",
    "lerna:dev:ui:r63b": "lerna run dev --parallel --scope @cyclone/client-theme-r63b",
    "lerna:dev:ui:r63d": "lerna run dev --parallel --scope @cyclone/client-theme-r63d",
    "lerna:dev:ui:mobile": "lerna run dev --parallel --scope @cyclone/client-theme-m01"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrchirub/Cyclone.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mrchirub/Cyclone/issues"
  },
